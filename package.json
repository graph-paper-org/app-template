{
  "name": "graph-paper-app",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup --config",
    "build-storybook": "build-storybook --static-dir ./public --config-dir .storybook",
    "dev": "rollup --config --watch",
    "format": "prettier --plugin-search-dir=. --write .",
    "format:check": "prettier --plugin-search-dir=. --check .",
    "lint": "eslint .",
    "start": "sirv public",
    "storybook": "start-storybook --static-dir ./public -p 6006",
    "test": "npm-run-all format:check lint test:jest",
    "test:jest": "jest tests --no-cache"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@graph-paper/core": "0.0.0-alpha.15",
    "@graph-paper/datagraphic": "0.0.0-alpha.15",
    "@graph-paper/elements": "0.0.0-alpha.15",
    "@graph-paper/guides": "0.0.0-alpha.15",
    "@rollup/plugin-commonjs": "15.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@storybook/addon-knobs": "6.0.26",
    "@storybook/addon-storysource": "6.0.26",
    "@storybook/svelte": "6.5.4",
    "babel-loader": "8.1.0",
    "eslint": "7.10.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "6.13.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-svelte3": "2.7.3",
    "jest": "26.5.2",
    "npm-run-all": "4.1.5",
    "page": "1.11.6",
    "prettier": "2.1.2",
    "prettier-plugin-svelte": "1.4.1",
    "rollup": "2.29.0",
    "rollup-plugin-livereload": "2.0.0",
    "rollup-plugin-postcss": "3.1.8",
    "rollup-plugin-svelte": "6.0.1",
    "rollup-plugin-terser": "7.0.2",
    "svelte": "3.29.0",
    "svelte-loader": "2.13.6"
  },
  "dependencies": {
    "sirv-cli": "1.0.6"
  }
}
